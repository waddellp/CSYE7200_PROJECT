# Routes
# This file defines all application routes (Higher priority routes first)
# https://www.playframework.com/documentation/latest/ScalaRouting
# ~~~~

# An example controller showing a sample home page
GET     /                                   controllers.HomeController.index
#GET     /lookup                             controllers.LookupController.lookup
#GET     /lookup                             controllers.LookupController.clear
#POST    /lookup                             controllers.LookupController.historicalLookup
#GET     /lookup                             controllers.HomeController.lookup
GET     /topten                             controllers.HomeController.topten
GET     /lookup                             controllers.LookupController.lookup
POST    /lookupPost                         controllers.LookupController.message
GET     /lookupresult                       controllers.LookupController.lookupresult

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
